<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cloud.ibm.banking.IBMBanking.Persistence.Entity.AccountInformation0Entity"
           table="account_information0" schema="banking">
        <id name="accountName">
            <column name="accountName" sql-type="varchar(15)" length="15"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="balance">
            <column name="balance" sql-type="float" precision="-1"/>
        </property>
        <property name="manageType">
            <column name="manageType" sql-type="int(11)"/>
        </property>
        <property name="lastDealTime">
            <column name="lastDealTime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="payingPassword">
            <column name="payingPassword" sql-type="int(6)"/>
        </property>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cloud.ibm.banking.IBMBanking.Persistence.Entity.AccountInformation0Entity"
           table="account_information0" schema="banking">
        <composite-id>
            <key-property name="id" column="id"/>
            <key-property name="identity" column="identity"/>
        </composite-id>
        <property name="password" column="password"/>
        <property name="balance" type="java.lang.Float" column="balance"/>
        <property name="manageType" column="manage_type"/>
        <property name="payingPassword" column="paying_password"/>
        <property name="lastDealTime" column="last_deal_time"/>
    </class>
</hibernate-mapping>